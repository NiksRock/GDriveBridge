{
  "name": "GDriveBridge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write .",
    "prepare": "husky",
    "clean": "powershell -ExecutionPolicy Bypass -File clean.ps1",
